    "grunt-postcss": "0.9.0",
    "grunt-sass": "3.1.0",
    "mocha": "8.4.0",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "15.1.0",
    "rewire": "2.5.2",
    "sass": "1.52.3",
    "silvermine-serverless-utils": "git+https://github.com/silvermine/serverless-utils.git#910f1149af824fc8d0fa840878079c7d3df0f414",
